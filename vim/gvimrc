if has("gui_running")

  " 隐藏工具栏
  set guioptions-=T
  " 隐藏滚动条
  set guioptions-=r
  " 隐藏菜单栏
  set guioptions-=m
  " 启动窗口大小
  set lines=30 columns=110
  " 启动窗口位置x,y
  winpos 150 50
  " 字体
  " set guifont=Monospace\ 13
  set guifont=Monaco\ 11
  "set guifont=Consolas\ 11

endif
